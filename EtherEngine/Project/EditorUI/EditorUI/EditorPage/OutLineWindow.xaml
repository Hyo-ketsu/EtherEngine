<UserControl x:Class="EditorUI.OutLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ma="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:av="https://github.com/Dirkster99/AvalonDock"
        xmlns:local="clr-namespace:EditorUI"
        mc:Ignorable="d">
   <Grid>
      <local:OldTreeView x:Name="ObjectList" SelectedItemChanged="SelectedItemChanged" KeyDown="ObjectListKeyDown">
      </local:OldTreeView>
   </Grid>
   
   <UserControl.ContextMenu>
      <ContextMenu>
         <MenuItem Command="{Binding CreateCommand}">
            <MenuItem.Header>
               <local:EditorText EN="Create GameObject"
                                 JP="ゲームオブジェクト作成"/>
            </MenuItem.Header>
         </MenuItem>
         <MenuItem Command="{Binding DeleteCommand}">
            <MenuItem.Header>
               <local:EditorText EN="Delete GameObject"
                                 JP="ゲームオブジェクト削除"/>
            </MenuItem.Header>
         </MenuItem>
      </ContextMenu>
   </UserControl.ContextMenu>
</UserControl>
